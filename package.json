{
	"name": "agentk",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:server": "nodemon --watch server.ts --exec tsx server.ts",
		"build": "next build",
		"build:server": "tsc server.ts --outDir dist",
		"start": "next start",
		"start:server": "node dist/server.js",
		"lint:next": "next lint",
		"lint": "biome check . && eslint . --max-warnings 0",
		"format": "biome format . --write && prettier -w .",
		"type-check": "tsc --noEmit",
		"lint-staged": "lint-staged",
		"prepare": "husky"
	},
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@monaco-editor/react": "^4.7.0",
		"@prisma/extension-accelerate": "^1.3.0",
		"@reduxjs/toolkit": "^2.2.6",
		"@xterm/addon-fit": "^0.10.0",
		"@xterm/addon-web-links": "^0.11.0",
		"@xterm/xterm": "^5.5.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.5.0",
		"drizzle-orm": "^0.43.1",
		"express": "^5.1.0",
		"golden-layout": "^2.6.0",
		"ioredis": "^5.6.1",
		"lucide-react": "^0.511.0",
		"monaco-editor": "^0.52.2",
		"next": "15.3.2",
		"openai": "^4.100.0",
		"pg": "^8.16.0",
		"postgres": "^3.4.5",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-icons": "^5.5.0",
		"react-redux": "^9.1.2",
		"socket.io": "^4.8.1",
		"socket.io-client": "^4.8.1",
		"tailwind-merge": "^3.3.0",
		"y-websocket": "^3.0.0",
		"yjs": "^13.6.27"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@eslint/eslintrc": "^3",
		"@next/eslint-plugin-next": "^15.3.2",
		"@prisma/client": "^6.8.2",
		"@tailwindcss/postcss": "^4",
		"@types/express": "^5.0.2",
		"@types/node": "^20",
		"@types/pg": "^8.15.2",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/socket.io": "^3.0.1",
		"eslint": "^9",
		"eslint-config-next": "15.3.2",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.0.0",
		"nodemon": "^3.1.10",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.8.2",
		"tailwindcss": "^4",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.4",
		"turbo": "^2.5.3",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx}": ["biome format --write", "eslint --cache --fix"],
		"*.{json,md,css,scss,mdx}": ["biome format --write"]
